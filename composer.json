{
    "name": "alisqi/twigqi",
    "description": "Static code analysis for Twig templates",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen Versteeg"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlisQI\\TwigQI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlisQI\\TwigQI\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "twig/twig": "^3.13",
        "phpdocumentor/reflection-docblock": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
