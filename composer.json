{
    "name": "alisqi/twig-stan",
    "description": "Static code analysis for Twig templates",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen Versteeg"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlisQI\\TwigStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlisQI\\TwigStan\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "twig/twig": "3.*-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
